classDiagram
    class Kunde {
        +bestellen(mitarbeiter: Mitarbeiter): Pommes
        +antwort_geben(): bool
    }

    class Mitarbeiter {
        +mayo_fragen(kunde: Kunde): bool
        +pommes_vorbereiten(): Pommes
    }

    class Pommes {
        +vorbereiten(): void
    }

    Kunde --> Mitarbeiter : "interagiert mit"
    Mitarbeiter --> Pommes : "bereitet vor"